@model IEnumerable<SupportDesk.Models.SupportGroup>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Overseer_Employee_ID)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Employee.F_Name)
                @Html.DisplayFor(modelItem => item.Employee.L_Name)
            </td>
            <td>
                @Html.Partial("_Actions", item)
            </td>
        </tr>
    }

</table>